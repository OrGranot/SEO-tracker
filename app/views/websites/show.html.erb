<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @website.name %>
</p>

<p>
  <strong>Url:</strong>
  <%= @website.url %>
</p>

<p>
  <strong>User:</strong>
  <%= @website.user_id %>
</p>

<br />


    <table class="table">
  <thead>
    <tr>
      <th scope="col">Keyword</th>
      <th scope="col">22.3</th>
      <th scope="col">23.3</th>
      <th scope="col">24.3</th>
      <th scope="col">25.3</th>
    </tr>
  </thead>
  <tbody>
    <% @keywords.each do |keyword| %>
      <tr>
        <th>
          <%= keyword.key_string %>
        </th>
        <% keyword.searches.first(4).each do |search| %>
          <td><%= search.rank %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "search", website_search_add_path(@website) %>
<%= link_to "add keyword", website_keywords_add_path(@website) %>


<%= link_to 'Edit', edit_website_path(@website) %> |
<%= link_to 'Back', websites_path %>
